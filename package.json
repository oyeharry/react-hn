{
  "name": "react-hn",
  "version": "1.0.0",
  "description": "A React implementation of Hacker News feed.",
  "main": "index.js",
  "author": "Harminder",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node index.js",

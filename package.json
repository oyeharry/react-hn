{
  "name": "react-hn",
  "version": "1.0.0",
  "description": "A React implementation of Hacker News feed.",
  "main": "index.js",
  "author": "Harminder",
  "license": "MIT",
  "scripts": {
    "dev": "run-s dev:webpack dev:start",
    "dev:start": "run-p watch:dev:webpack watch:dev:server",
    "test": "jest",
    "build": "run-s clean static:copy prod:webpack",
    "start": "node ./build/server.js",
    "eslint": "eslint src/ --ext .js",
    "clean": "shx rm -rf build",
    "watch:dev:webpack": "webpack --config webpack.config.babel.js --mode=development --watch",
    "watch:dev:server": "nodemon --watch build ./build/server.js",
    "test:build": "run-s eslint test build",
    "static:copy": "shx cp -r 'src/static' build",
    "prod:webpack": "webpack --config webpack.config.babel.js --mode=production",
    "dev:webpack": "webpack --config webpack.config.babel.js --mode=development"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/react-hooks": "^3.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^9.0.1",
    "react-test-renderer": "^16.13.1",
    "regexparam": "^1.3.0",
    "shx": "^0.3.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cross-fetch": "^3.0.4",
    "date-fns": "^2.12.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.3",
    "express-sslify": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "serialize-javascript": "^3.0.0",
    "serve-favicon": "^2.5.0",
    "styled-components": "^5.1.0",
    "styled-reset": "^4.1.3",
    "styled-system": "^5.1.5",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
